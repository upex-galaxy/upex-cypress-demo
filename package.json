{
	"name": "upex-cypress",
	"version": "12",
	"description": "Cypress Automation",
	"main": "index.js",
	"scripts": {
		"test": "cypress open",
		"rm:reports": "rm reports/* || true",
		"file": "cypress run --browser chrome --reporter cypress-multi-reporters --reporter-options configFile=jsconfig.json --spec",
		"regression": "cypress run --browser chrome --reporter cypress-multi-reporters --reporter-options configFile=jsregression.json",
		"regression:report": "cypress run --browser chrome --reporter mochawesome --reporter-options reportDir=mochawesome-report,overwrite=false,html=false,json=true",
		"report:xml": "jrm reports/regression-report.xml \"reports/*.xml\"",
		"report:json": "mochawesome-merge mochawesome-report/*.json>mochawesome.json",
		"report:html": "marge mochawesome.json"
	},
	"keywords": [
		"cypress",
		"automation",
		"testing"
	],
	"author": "Uper",
	"license": "ISC",
	"type": "module",
	"devDependencies": {
		"@4tw/cypress-drag-drop": "2.2.2",
		"@badeball/cypress-cucumber-preprocessor": "12.2.0",
		"@bahmutov/cypress-esbuild-preprocessor": "2.1.5",
		"@esbuild-plugins/node-modules-polyfill": "0.1.4",
		"@faker-js/faker": "7.6.0",
		"cy-verify-downloads": "0.1.11",
		"cypress": "12.0.2",
		"cypress-downloadfile": "1.2.3",
		"cypress-file-upload": "5.0.8",
		"cypress-mochawesome-reporter": "3.2.3",
		"cypress-multi-reporters": "1.6.2",
		"cypress-plugin-api": "2.6.1",
		"cypress-recurse": "1.24.0",
		"cypress-wait-until": "1.7.2",
		"cypress-xpath": "2.0.1",
		"junit-report-merger": "4.0.0",
		"mocha": "10.2.0",
		"mocha-junit-reporter": "2.2.0",
		"mochawesome": "7.1.3",
		"mochawesome-merge": "4.2.2",
		"mochawesome-report-generator": "6.2.0"
	}
}
